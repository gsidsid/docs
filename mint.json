{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Prefix",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "modeToggle": {
    "default": "light"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#FBCFAE",
    "light": "#F78757",
    "dark": "#0D9373",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@prefix.app"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://prefix.app/dashboard"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://prefix.app/docs"
    },
    {
      "name": "Community",
      "icon": "slack",
      "url": "https://mintlify.com/community"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://prefix.app/blog"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
      "group": "Essentials",
      "pages": ["essentials/markdown", "essentials/code", "essentials/images", "essentials/settings", "essentials/navigation"]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/prefixrpa",
    "github": "https://github.com/prefix-app",
    "linkedin": "https://www.linkedin.com/company/prefix-software"
  }
}
